@page

@using ElleChristine.Web.Pages

@model GalleryModel

@{
    ViewData["Title"] = "Gallery - Elle Christine";
}

<partial name="Partials/Gallery/_PageTitle" />

<section>

    <div class="container">


        <div class="section-heading">
            <h2 id="pictures">Pictures</h2>
            <p class="w-md-75 w-lg-55">A collection of the latest pictures.</p>
        </div>

        <div class="row portfolio-gallery mt-n1-9">

            @foreach (var photo in Model.Photos)
            {
                <partial name="Partials/Gallery/_Photo" model="photo" />
            }

        </div>
    </div>

</section>
